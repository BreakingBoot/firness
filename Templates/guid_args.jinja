{#
    Generate list of guids
#}    
    {{ arg_type }} {{ arg_key }};
    UINT8 *{{ arg_key }}_choice = NULL;
    ReadBytes(&Input, 1, &{{ arg_key }}_choice);
    switch({{ arg_key }}_choice % {{ arguments | length }}) {
        {%- for argument in arguments %}
        case {{ loop.index }}:
            {{ arg_key }} = &{{ argument.variable }};
            break;
        {%- endfor %}
    }